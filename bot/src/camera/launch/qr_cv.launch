<launch>
  <arg name="mode" default="0" />
  <param name="mode" type="string" value="$(arg mode)"/>
  
  <node pkg="camera" type="image_input_cv.py" name="image_input" required="true" args="--mode=$(arg mode)" output="screen" />
  <node pkg="camera" type="qr_common.py" name="qr_service" output="screen" />
</launch>